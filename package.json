{
  "name": "hull-connectors",
  "version": "0.1.40-monorepo",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/connectors/*",
      "packages/hull-connector-framework",
      "packages/hullrepl",
      "packages/hull-vm",
      "packages/hull-webhooks",
      "packages/hull-lightweight",
      "packages/hull-sql-importer",
      "packages/hull-sql-exporter",
      "packages/hull",
      "packages/hull-client",
      "packages/minihull"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint packages --fix"
    ]
  },
  "license": "MIT",
  "scripts": {
    "duplicated-dependencies": "syncpack-list-mismatches -s 'packages/*/package.json' -s 'packages/connectors/*/package.json'",
    "build": "sh scripts/build-connectors.sh",
    "repl": "NODE_ENV=development sh scripts/repl.sh",
    "clean": "sh scripts/clean-connectors.sh",
    "start": "PORT=8080 bash scripts/start-connector.sh",
    "combined": "PORT=8082 sh scripts/combined-start.sh",
    "combined:lightweight": "PORT=8080 sh scripts/combined-start.sh",
    "stop": "sh scripts/stop-dev-connector.sh",
    "ngrok": "sh scripts/ngrok.sh",
    "dev": "NODE_ENV=development sh scripts/start-dev-connector.sh",
    "client": "NODE_ENV=development sh scripts/start-client-connector.sh",
    "test:run": "sh scripts/run-test.sh",
    "test:watch": "sh scripts/watch-test.sh",
    "test": "yarn test:lint && yarn test:jest && yarn test:workspaces",
    "test:flow": "flow check",
    "test:lint": "eslint packages",
    "test:jest": "jest --maxWorkers=2",
    "test:workspaces": "yarn wsrun -s --fast-exit test",
    "test:ci": "yarn test:lint && yarn test:jest && yarn wsrun --fast-exit --parallel --concurrency=2 test",
    "heroku-postbuild": "yarn build",
    "heroku-prebuild": "rm -rf node_modules"
  },
  "engines": {
    "node": "^12",
    "npm": "6.4.x",
    "yarn": "^1.10.x"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chai-subset": "^1.6.0",
    "chalk": "^4.1.0",
    "documentation": "^13.1.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "flow-bin": "^0.145.0",
    "flow-typed": "^3.2.1",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.0",
    "nock": "^13.0.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-hot-loader": "^4.13.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.3.0",
    "syncpack": "^5.7.11",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.17",
