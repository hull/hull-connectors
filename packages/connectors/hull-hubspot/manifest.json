{
  "name": "Hubspot",
  "description": "Two-way Hubspot integration for Contacts & Companies",
  "source": "hubspot",
  "logo": "logo.png",
  "picture": "picture.png",
  "version": "0.1.38-monorepo",
  "tags": [
    "outgoing",
    "incoming",
    "batch",
    "batch-accounts",
    "oneColumn",
    "smart-notifier",
    "first-install",
    "single-install",
    "kraken-exports"
  ],
  "settings": [],
  "private_settings": [
    {
      "name": "fetch_visitors",
      "title": "Resolve Contact ID from Visitor UTK",
      "description": "Given a UTK, resolve the associated Contact ID from Hubspot",
      "type": "boolean",
      "default": false
    },
    {
      "name": "synchronized_visitor_segments",
      "title": "Visitor Segment Filter",
      "description": "Given a UTK, resolve the associated contact ID from Hubspot when users match these segments",
      "type": "array",
      "format": "segment",
      "options": {
        "standard": {
          "ALL": "All Users"
        }
      },
      "default": []
    },
    {
      "name": "send_events",
      "title": "Send Events",
      "description": "Should Send Events",
      "type": "boolean",
      "default": false
    },
    {
      "name": "outgoing_user_events",
      "title": "Sends Events to Hubspot",
      "format": "event",
      "type": "array",
      "default": [],
      "options": {
        "standard": {
          "all_events": "All Events"
        }
      }
    },
    {
      "name": "ignore_deleted_users",
      "title": "Ignore Deleted Contacts",
      "description": "If a contact has been deleted in Hubspot, do not send that user to Hubspot from Hull.",
      "type": "boolean",
      "default": true
    },
    {
      "name": "ignore_deleted_accounts",
      "title": "Ignore Deleted Companies",
      "description": "If an company has been deleted in Hubspot, do not send that account to Hubspot from Hull.",
      "type": "boolean",
      "default": true
    },
    {
      "name": "mark_deleted_contacts",
      "title": "Mark Deleted Contacts",
      "description": "If a contact has been deleted in Hubspot, mark it as such in Hull.",
      "type": "boolean",
      "default": true
    },
    {
      "name": "mark_deleted_companies",
      "title": "Mark Deleted Companies",
      "description": "If a company has been deleted in Hubspot, mark it as such in Hull.",
      "type": "boolean",
      "default": true
    },
    {
      "name": "oauth",
      "title": "Credentials",
      "description": "Authenticate with Hubspot",
      "format": "oauth",
      "type": "string",
      "handler": "oauth",
      "url": "/auth",
      "options": {
        "name": "Hubspot",
        "strategy": {
          "scope": [
            "oauth",
            "contacts",
            "timeline",
            "content"
          ]
        }
      }
    },
    {
      "name": "last_fetch",
      "type": "string",
      "title": "Last Fetched",
      "format": "information",
      "url": "/last-fetched-status"
    },
    {
      "name": "incoming_user_title",
      "title": "User Incoming Setup",
      "description": "How we capture Hubspot Contacts as users",
      "format": "title",
      "type": "string"
    },
    {
      "name": "incoming_user_claims",
      "title": "User Identity",
      "type": "array",
      "format": "table",
      "items": {
        "type": "object",
        "properties": {
          "service": {
            "title": "Hubspot Identifier",
            "type": "string",
            "format": "select",
            "options": {
              "allowCreate": false,
              "loadOptions": "/schema/incoming_user_claims"
            }
          },
          "hull": {
            "title": "Hull Identifier",
            "enum": [
              "external_id",
              "email"
            ]
          },
          "required": {
            "type": "boolean",
            "title": "required?",
            "format": "select"
          }
        }
      },
      "default": [
        {
          "hull": "email",
          "service": "$['identity-profiles'][*].identities[?(@.type === 'EMAIL')].value",
          "required": false
        },
        {
          "hull": "email",
          "service": "properties.email.value",
          "required": false
        }
      ]
    },
    {
      "name": "incoming_user_attributes",
      "title": "Incoming User Attribute Mapping",
      "description": "Define which attributes are captured from Hubspot",
      "type": "array",
