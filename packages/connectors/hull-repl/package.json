{
  "name": "hull-repl",
  "description": "Send test data to debug your flows",
  "version": "0.1.34-monorepo",
  "homepage": "https://github.com/hull/hull-connectors",
  "scripts": {
    "test": "exit 0; echo 'No Tests here' Sad Panda",
    "testx": "yarn test:unit && yarn test:integration",
    "test:unit": "NODE_ENV=test mocha --exit --require ../../../root-babel-register.js -R spec ./test/unit",
    "test:integration": "NODE_ENV=test mocha --exit --require ../../../root-babel-register.js -R spec ./test/integration/{*.js,**/*.js}"
  },
  "dependencies": {
    "domready": "^1.0.8"
  },
  "devDependencies": {}
}
