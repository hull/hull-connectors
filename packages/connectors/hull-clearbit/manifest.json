  "subscriptions": [
    {
      "url": "/smart-notifier",
      "conditions": {
        "channels": {
          "only": [
            "user:update",
            "account:update"
          ]
        }
      },
      "channels": [
        {
          "channel": "user:update",
          "handler": "updateUser"
        },
        {
          "channel": "account:update",
          "handler": "updateAccount"
        }
      ]
    }
  ],
  "incoming": [
    {
      "url": "/clearbit-enrich",
      "handler": "webhookHandler"
    }
  ],
  "json": [
    {
      "url": "/prospect",
      "handler": "prospectHandler"
    },
    {
      "url": "/schema/company_properties",
